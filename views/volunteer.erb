<h2> volunteer: <%= @volunteer.name %></h2>
<br><br>

<form action="/projects/<%= @project.id %>/volunteers/<%=@volunteer.id %>" method="post">
<input name="_method" type="hidden" value="patch">
  <div class="form-group">    
    <label for="name">Edit Volunteer:</label>
    <input id="name" name="name" class="form-control" type="text" required>
  </div>
  <button type="submit" class="btn btn-primary">Update Volunteer</button> 
</form> 


<a href="/projects/<%= @project.id %>" class="btn btn-info">Return to Project</a> 

